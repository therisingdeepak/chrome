{
"background": {
    "persistent": false,
    "page": "background.html"
  },
"options_page": "options.html",
"manifest_version":2,
  "name": "Gmail for Salesforce",
  "version": "1.0",
"permissions": [
   "history", "tabs", "http://*/*", "alarms",
    "webNavigation","notifications",
    "*://*.google.com/","https://*/*"
  ],
  "description": "gmail extension for data.com by Deepak Dhakal.",
  "browser_action": {
    "default_icon": "icon.png",
     "default_popup": "popup.html"
  }
}